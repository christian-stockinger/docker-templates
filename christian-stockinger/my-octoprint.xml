  <Config Name="Printer" Target="" Default="" Mode="" Description="Printer Device&#13;&#10;" Type="Device" Display="always" Required="false" Mask="false">/dev/ttyUSB0:/dev/ttyUSB0</Config>
  <Config Name="Data" Target="/home/octoprint/.octoprint" Default="" Mode="rw" Description="Container Path: /home/octoprint/.octoprint" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/OctoPrint/</Config>
